class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
       int n=nums.length;
       int res=0;
       int maxCount=0;
       for(int i=0;i<n;i++){
           if(nums[i]==1){
             res++;
           }else{
               maxCount=Math.max(res,maxCount);
               res=0;
           }
       }
       return res>maxCount? res:maxCount;
    }
}
